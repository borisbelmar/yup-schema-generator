{
  "name": "@borisbelmar/yup-schema-generator",
  "version": "1.0.0",
  "description": "Generate a Yup schema from a JSON object",
  "scripts": {
    "build": "tsup ./src/index.ts --format esm,cjs --dts --minify",
    "watch": "tsup ./src/index.ts --format esm,cjs --dts --minify --watch",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix",
    "clean": "rm -rf node_modules && rm -rf dist",
    "test": "jest",
    "test:script": "ts-node --project tsconfig.json ./src/tests/testExecution.ts",
    "ci": "yarn --freeze-lockfile",
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "prepare": "husky install"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "author": "Boris Belmar <borisbelmarm@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "yup": "1.0.2"
  },
  "devDependencies": {
    "@types/jest": "29.2.3",
    "@types/node": "18.11.11",
    "@types/nodemailer": "6.4.6",
    "@types/ramda": "0.28.20",
    "@types/yup": "0.32.0",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "eslint": "8.28.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.2",
    "jest": "29.5.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "tsup": "6.5.0",
    "typescript": "4.9.3"
  }
}
